  "author": {
    "name": "Mathieu Leblanc",
    "email": "spiky.ing@gmail.com",
    "url": "https://github.com/spikying"
  },
  "publisher": "spikying",
  "bin": {
    "chptr": "./bin/run"
  },
  "bugs": "https://github.com/spikying/chptr/issues",
  "dependencies": {
    "@braintree/sanitize-url": "3.1.0",
