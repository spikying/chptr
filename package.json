{
  "author": "Mathieu Leblanc",
  "bin": {
    "chptr": "./bin/run.js"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.0.0",
    "@oclif/core": "^3",
    "@oclif/errors": "^1.3.6",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "chalk": "^4.1.2",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "debug": "^4.3.4",
    "deep-diff": "^1.0.2",
    "diff": "^5.2.0",
    "glob": "^10.3.10",
    "inquirer": "^8.2.6",
    "inquirer-fuzzy-path": "^2.3.0",
    "js-yaml": "^4.1.0",
    "latinize": "^1.0.0",
    "moment": "^2.30.1",
    "node-notifier": "^10.0.1",
    "sanitize-filename": "^1.6.3",
    "simple-git": "^3.22.0",
    "tmp-promise": "^3.0.3",
    "typescript-ioc": "^3.2.2",
    "validator": "^13.11.0",
    "word-regex": "^0.1.2",
    "yaml": "^2.4.1"
  },
  "description": "CLI tool to handle separate Markdown files with special markup notations and export as a complete document",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/convict": "^6.1.6",
    "@types/debug": "^4.1.12",
    "@types/deep-diff": "^1.0.5",
    "@types/diff": "^5.0.9",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/latinize": "^0.2.18",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18",
    "@types/node-notifier": "^8.0.5",
    "@types/tmp": "^0.2.6",
    "@types/validator": "^13.11.7",
    "chai": "^4",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^3.17.2",
    "prettier": "^3.2.5",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/spikying/chptr",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "chptr",
  "oclif": {
    "bin": "chptr",
    "dirname": "chptr",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "spikying/chptr",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "clean": "shx rm -rf dist && shx rm tsconfig.tsbuildinfo && shx rm oclif.manifest.json",
    "format": "prettier **/*.ts --write",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --fix --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "run-dev": "npm run build && env DEBUG=* node ./bin/dev --help",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "1.0.0",
  "bugs": "https://github.com/spikying/chptr/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
