    "@types/debug": "4.1.4",
    "@types/deep-diff": "1.0.0",
    "@types/diff": "4.0.2",
    "@types/glob": "7.1.1",
    "@types/inquirer": "6.0.3",
    "@types/js-yaml": "3.12.1",
    "@types/json5": "0.0.30",
    "@types/latinize": "0.2.15",
    "@types/mkdirp": "0.5.2",
    "@types/node-notifier": "5.4.0",
    "@types/sanitize-filename": "1.1.28",
    "@types/tmp": "0.1.0",
    "@types/uuid": "3.4.5",
    "@types/validator": "10.11.1",
    "@types/yaml": "1.0.2",
    "chalk": "2.4.2",
    "cli-ux": "5.2.1",
    "comment-json": "1.1.3",
    "convict": "5.0.0",
    "debug": "4.1.1",
    "deep-diff": "1.0.2",
    "diff": "4.0.1",
    "glob": "7.1.4",
    "inquirer": "6.3.1",
    "inquirer-fuzzy-path": "2.0.3",
    "js-yaml": "3.13.1",
    "json5": "2.1.0",
    "latinize": "0.4.0",
    "minimatch": "3.0.4",
    "mkdirp": "0.5.1",
    "moment": "2.24.0",
    "node-notifier": "5.4.0",
    "sanitize-filename": "1.6.1",
    "simple-git": "1.119.0",
    "tmp-promise": "2.0.1",
    "tslib": "1.9.3",
    "validator": "11.1.0",
    "word-regex": "0.1.2",
    "yaml": "1.6.0"
